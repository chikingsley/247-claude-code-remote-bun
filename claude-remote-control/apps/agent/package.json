{
  "name": "247-agent",
  "version": "2.44.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "start": "bun dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "bunx --bun vitest run",
    "test:unit": "bunx --bun vitest run tests/unit",
    "test:integration": "bunx --bun vitest run tests/integration",
    "test:watch": "bunx --bun vitest",
    "test:coverage": "bunx --bun vitest run --coverage"
  },
  "dependencies": {
    "247-shared": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "http-proxy": "^1.18.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "web-push": "^3.6.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.0",
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^22.10.5",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.4",
    "@types/ws": "^8.5.0",
    "execa": "^9.6.1",
    "supertest": "^7.2.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
