{
  "name": "claude-remote-control",
  "version": "0.1.0",
  "private": true,
  "description": "Web terminal access to Claude Code from anywhere",
  "author": "Stan",
  "license": "MIT",
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules",
    "test": "turbo test",
    "test:unit": "turbo test:unit",
    "test:integration": "turbo test:integration",
    "test:coverage": "turbo test -- --coverage",
    "dev:web": "pnpm --filter @claude-remote/web dev",
    "dev:agent": "pnpm --filter @claude-remote/agent dev",
    "db:generate": "pnpm --filter @claude-remote/web db:generate",
    "db:migrate": "pnpm --filter @claude-remote/web db:migrate",
    "db:push": "pnpm --filter @claude-remote/web db:push",
    "db:studio": "pnpm --filter @claude-remote/web db:studio"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
